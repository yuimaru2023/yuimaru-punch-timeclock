{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
          "destination": "/administrator/[id]/index.html",
          "regex": "^/administrator/([^/]+?)(?:/)?$"
        }
    ]
  }
}
